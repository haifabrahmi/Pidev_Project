<AnchorPane id="AnchorPane" prefHeight="664.0" prefWidth="600.0" style="-fx-background-color: #FCAE1E;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.EditPublicationController">
